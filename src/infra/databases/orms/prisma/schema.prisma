generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model LocationWrap {
  id Int  @default(autoincrement()) @id
  uuid String @db.Uuid() @unique() @default(uuid())
  geolocationX Decimal @db.Decimal() @map("geo_location_x")
  geolocationY Decimal @db.Decimal() @map("geo_location_y")

  @@map("location_wrap")
}
